		"events": {
			"scythe:attacked": {
				"add": {
					"component_groups": [
						"scythe:onAttack"
					]
				},
				"remove": {
					"component_groups": [
						"scythe:reset_skinid"
					]
				}
			},
			"scythe:guiopen": {
				"add": {
